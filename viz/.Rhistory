#shapeData <- sf::st_read(dsn = "./Shapefile_PLZ", layer = "PLZO_PLZ", as_tibble = F)
#shapeData <- sf::st_read("./Shapefile_PLZ", "PLZO_PLZ", stringsAsFactor = F)
